	function jumpbackAgreement() { 
		jumpback(); 
	} 
	function jumpback() { 
    	top.location.replace('/' + apttusAgmtId);
	} 
</script> 
<body onload="jumpbackAgreement();"  class="home  editPage"> 
	<center> 
		<img src="/img/waiting_dots.gif" alt="Please wait..." title="Please wait..." height="20" width="196"> 
		<h2>Please wait... sending agreement</h2> 
	</center> 
<script type="text/javascript">document.write('<img src="https://www.echosign.com/salesforce/importAgreement?namespace=echosign_dev1&Agreement_ID=' + esAgmtId + '&Version=6&session=' + userSession + '&server=' + sfServerURL + '&rand=' + Math.random() + '&attachmentIds=' + attachmentIds + '&signerFields=' + recipients + '">');</script> 
