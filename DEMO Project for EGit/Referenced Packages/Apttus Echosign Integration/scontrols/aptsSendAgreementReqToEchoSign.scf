<script language="javascript" type="text/javascript"> 

var esAgmtId = "{!$Request.esAgmtId}";
var apttusAgmtId = "{!$Request.agmtId}";
var attachmentIds = "{!$Request.attachmentIds}";
var userSession = "{!User.Session_ID}";
var sfServerURL = "{!API.Partner_Server_URL_70}";
